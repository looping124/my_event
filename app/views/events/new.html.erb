<h1>Events#new</h1>
<p>Find me in app/views/events/new.html.erb</p>
<div class="w-50 m-2 ">
  <%= simple_form_for @event do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.date_select  :start_date,{minute_step: 5} %>
    <%= f.time_select  :start_date,{minute_step: 5} %>
    <%= f.input :duration %>
    <%= f.input :price %>
    <%= f.input :location %>
   <%= f.button :submit ,class:'btn btn-dark mt-3'%>
  <% end %>
</div>